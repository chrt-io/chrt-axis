{
  "name": "chrt-axis",
  "version": "0.0.54",
  "description": "Axis component for Chrt",
  "keywords": [
    "chrt",
    "charts",
    "axis",
    "visualization"
  ],
  "type": "module",
  "main": "src/index.js",
  "module": "src/index.js",
  "jsdelivr": "dist/chrt-axis.min.js",
  "unpkg": "dist/chrt-axis.min.js",
  "exports": {
    "umd": "./dist/chrt-axis.min.js",
    "default": "./dist/chrt-axis.esm.js"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx"
    }
  },
  "scripts": {
    "test": "TZ=UTC jest",
    "build": "rollup -c",
    "postversion": "rollup -c",
    "lint": "eslint src --fix",
    "watch": "npm-watch",
    "dev": "budo test/chrts/index.js:bundle.js --live --dir test/chrts  -- -t [ babelify --extensions .js --presets [ @babel/preset-env ] --global true  ]",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:chrt-io/chrt-axis.git"
  },
  "author": "chrt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chrt-io/chrt-axis/issues"
  },
  "homepage": "chrt.io",
  "dependencies": {
    "chrt-object": "0.0.16"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "babel-jest": "^29.5.0",
    "babel-plugin-root-import": "^6.6.0",
    "babelify": "^10.0.0",
    "budo": "^11.8.4",
    "chrt": "0.0.154",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "npm-watch": "^0.11.0",
    "rollup": "2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "watch": "^1.0.2"
  },
  "files": [
    "dist/**/*.js",
    "src/**/*.js"
  ]
}
